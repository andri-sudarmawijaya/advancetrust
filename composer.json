{
    "name": "feripratama/advancetrust",
    "type": "library",
    "description": "Advance trust",
    "keywords": [
        "feripratama",
        "advancetrust",
        "Role Management",
        "Permission"
    ],
    "homepage": "https://github.com/feripratama/advancetrust",
    "license": "MIT",
    "authors": [
        {
            "name": "Feri pratama",
            "email": "eroorsys@gmail.com",
            "homepage": "http://feripratama.id.vg",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "santigarcor/laratrust": "5.0.*",
        "that0n3guy/transliteration": "2.0.x",
        "laravelcollective/html": "^5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "feripratama\\advancetrust\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "feripratama\\advancetrust\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-vue"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
